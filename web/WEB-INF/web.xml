<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.SignupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>signin</servlet-name>
        <servlet-class>servlet.SigninServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signin</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>update</servlet-name>
        <servlet-class>servlet.UpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>course</servlet-name>
        <servlet-class>servlet.CourseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>course</servlet-name>
        <url-pattern>/course</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>returncourse</servlet-name>
        <servlet-class>servlet.ReturnCourseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>returncourse</servlet-name>
        <url-pattern>/returncourse</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>updateinformation</servlet-name>
        <servlet-class>servlet.UpdateInformationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateinformation</servlet-name>
        <url-pattern>/updateinformation</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>eavluate</servlet-name>
        <servlet-class>servlet.EvaluateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>eavluate</servlet-name>
        <url-pattern>/evaluate</url-pattern>
    </servlet-mapping>
</web-app>